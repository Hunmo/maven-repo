<?xml version="1.0" encoding="UTF-8"?><project>
  <parent>
    <artifactId>opennms-icmp-jni</artifactId>
    <groupId>org.opennms</groupId>
    <version>1.3.2</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>opennms-icmp-jni-macosx</artifactId>
  <packaging>jnilib</packaging>
  <name>OpenNMS ICMP JNI Library - MacOSX</name>
  <version>1.3.2</version>
  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>native-maven-plugin</artifactId>
        <extensions>true</extensions>
        <executions>
          <execution>
            <phase>generate-sources</phase>
            <goals>
              <goal>javah</goal>
            </goals>
            <configuration>
              <implementation>macos</implementation>
              <classNames>
                <className>org.opennms.protocols.icmp.IcmpSocket</className>
              </classNames>
            </configuration>
          </execution>
        </executions>
        <configuration>
          <compilerProvider>generic</compilerProvider>
          <compilerStartOptions>
            <compilerStartOption>-fPIC -Wall -Werror -O -D${platform.define} ${build.jni.compiler.arg}</compilerStartOption>
          </compilerStartOptions>
          <sources>
            <source>
              <directory>../src/main/native</directory>
              <fileNames>
                <fileName>IcmpSocket.c</fileName>
              </fileNames>
            </source>
          </sources>
          <linkerStartOptions>
            <linkerStartOption>-dynamiclib ${build.jni.compiler.arg} ${build.jni.linker.arg} -lc -ldl</linkerStartOption>
          </linkerStartOptions>
          <javahOS>${platform.javahOS}</javahOS>
          <jdkIncludePath>${platform.jdk.include.dir}</jdkIncludePath>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <distributionManagement>
    <status>deployed</status>
  </distributionManagement>
</project>